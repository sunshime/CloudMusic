.my {
  background-color: #1A1A1F;
  width: 100vw;
  height: 100vh;
  position: relative;
  overflow: hidden;
}

.headers {
  padding: 50rpx 50rpx 0rpx 50rpx;
  color: #FAFAFA;
}

.headers .avator {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;

}

.avator .img-icon {
  padding: 13rpx 20rpx;
  background-color: #D1D2D2;
  border-radius: 50%;
  font-size: 50rpx;
}

.avator .avator-text {
  margin-left: -30rpx;
}

.avator .avator-login {
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
  border: 1rpx solid #ccc;
}

.nav {
  padding: 50rpx 0 30rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.nav .list {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.nav .list .list-icon {
  font-size: 50rpx;
  font-weight: normal;
}

.nav .list:last-child .list-icon {
  font-size: 60rpx;
}

.nav .list .list-name {
  font-size: 26rpx;
}

.my .content {
  position: absolute;
  left: 0;
  top: 320rpx;
  width: 100vw;
  height: 100%;
  background-color: #FAFAFA;
  border-radius: 40rpx 40rpx 0 0;
  overflow: hidden;
  padding-top: 40rpx;
}

.content .ct-firt .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  padding-bottom: 30rpx;
}

.title-text {
  font-size: 32rpx;
  font-weight: 700;
  color: #333;
}

.title-icon {
  font-size: 42rpx;
  color: #A8A8A8;
}

.swipers {
  height: 340rpx;
}

.swipers .swiper-item .swiper-list {
  background-color: #E5E5E5;
  margin-right: 20rpx;
  padding: 0 30rpx;
  height: auto;
  border-radius: 15rpx;
  text-align: center;
  padding-bottom: 20rpx;
}

.swiper-list .list-tj {
  line-height: 80rpx;
  font-size: 26rpx;
  color: #AAAAAC;
}

.list-middle {
  display: flex;
  flex-direction: column;
  padding: 20rpx 0;
}

.list-middle .mdl-icon {
  font-size: 60rpx;
  color: #202028;
}

.mdl-name {
  font-size: 26rpx;
  color: #333335;
}


.list-desc {
  font-size: 26rpx;
  color: #AAAAAC;
  padding-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ct-sec {
  padding: 0 30rpx;
  color: #AFAFAF;
}

.ct-sec .sec-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.ct-sec .sec-title .sec-icon {
  font-size: 44rpx;
}

.sec-song {
  font-size: 32rpx;
  font-weight: 700;
  color: #333;
}

.sec-num {
  padding-left: 5rpx;
  font-size: 26rpx;
}

.sec-set-song {
  padding-left: 30rpx;
}

.sec-cont {
  margin: 20rpx 0rpx;
  border: 1rpx solid #AFAFAF;
  border-radius: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx 0;
}

.sec-cont-icon {
  font-size: 40rpx;
}
.sec-cont-text{
  font-size: 26rpx;
  padding-left: 10rpx;
  line-height: 50rpx;
}